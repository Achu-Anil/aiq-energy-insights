{
  "name": "aiq-energy-insights",
  "version": "1.0.0",
  "description": "Production-grade REST API for U.S. power plant energy generation insights",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "test": "jest --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --testPathIgnorePatterns=\"\\.e2e-spec\\.ts$\"",
    "test:watch": "jest --watch --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --testPathIgnorePatterns=\"\\.e2e-spec\\.ts$\"",
    "test:cov": "jest --coverage --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --testPathIgnorePatterns=\"\\.e2e-spec\\.ts$\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --testPathIgnorePatterns=\"\\.e2e-spec\\.ts$\"",
    "test:integration": "jest --testPathPatterns=\"\\.integration\\.spec\\.ts$\"",
    "test:e2e": "jest --testPathPattern=\"e2e-spec\" --runInBand",
    "test:redis": "ts-node src/scripts/test-redis.ts",
    "test:int:ci": "npm run db:migrate && npm run db:seed:int && npm run test:integration",
    "ingest": "ts-node src/scripts/ingest.ts",
    "ingest:tx": "ts-node src/scripts/ingest-transactional.ts",
    "seed": "ts-node prisma/seed.ts",
    "db:migrate": "prisma migrate deploy",
    "db:seed:int": "ts-node prisma/seed.integration.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.8",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.18.0",
    "@types/ioredis": "^4.28.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "prisma": "^6.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0"
  }
}
